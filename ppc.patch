diff --git a/net/ipv6/fib_sail_xdp.c b/net/ipv6/fib_sail_xdp.c
new file mode 100644
index 0000000..80176f4
--- /dev/null
+++ b/net/ipv6/fib_sail_xdp.c
@@ -0,0 +1,739 @@
+// SPDX-License-Identifier: GPL-2.0
+/* Copyright (c) 2018-19 MD Iftakharul Islam (Tamim) <mislam4@kent.edu>
+ *
+ *   This program is free software; you can redistribute it and/or
+ *   modify it under the terms of the GNU General Public License
+ *   as published by the Free Software Foundation; either version
+ *   2 of the License, or (at your option) any later version.
+ */
+
+#include <net/ip6_fib.h>
+
+/*The length of N16, P16 and C16 is 2^16*/
+#define LEVEL16_SIZE 65536
+
+/*Total number of chunks preallocated for B32/C32*/
+#define NUM_CHUNKS 64
+
+/*As we group each 64 elements, each chunk corresponds to 65536/64 elements*/
+#define NUM_ELEMS_PER_CHUNK 1024
+
+/*The size of B and C array. . Here each chunk consists of 64 groups*/
+#define BC_SIZE NUM_CHUNKS * 64
+
+/*Number of chunks in the last level (level 64)*/
+#define NUM_CHUNKS_LAST 400
+
+#define B64_SIZE NUM_CHUNKS_LAST * 64
+
+/*Number of small chunks*/
+#define SM_CHUNKS 16384
+
+/*The size of C40 and C48. Here each chunk consists of 4 groups*/
+#define BC_SIZE_SM SM_CHUNKS * 4
+
+
+/*chunk size is 2^16*/
+#define CNK_16 65536
+
+/*chunk size is 2^8*/
+#define CNK_8 256
+
+//FIXME: replace me with a smaller number
+/*memory preallocated for N and P array in level 24-64.*/
+#define ARR_SIZE NUM_CHUNKS * CNK_16
+
+#define ARR_SIZE_SM SM_CHUNKS * CNK_8
+
+/*Calculates the number of bits set to 1*/
+#define POPCNT(X) (hweight64(X))
+
+/*POPCNT of left-most N bits of X*/
+#define POPCNT_OFF(X, N) (hweight64(((1ULL << (N)) - 1) & (X)))
+
+static u16 calc_idx(struct bitmap_pc *c, u32 off)
+{
+	return c->popcnt + POPCNT_OFF(c->bitmap, off);
+}
+
+/*Converts a net_device to corresponding netdev_index*/
+static u8 get_netdev_index(struct PPC *p, struct net_device *dev)
+{
+	u8 i;
+
+	/*checks if the net_device is already seen; if yes then return the
+	 *corresponding index
+	 */
+	for (i = 0; i < p->netdev_count; i++) {
+		if (p->netdevs[i] == dev)
+			return i;
+	}
+	/*If the net_device is not previously seen, then add it to the array*/
+	p->netdevs[p->netdev_count++] = dev;
+	return p->netdev_count - 1;
+}
+
+/* Insert a new chunk to a level at chunk_id-1. Note that Chunk ID
+ * starts from 1, not 0. This function increments cnk_count of sail.
+ * This is why, it is passed by reference
+ */
+static int bc_arr_insert(struct bitmap_pc *B, struct bitmap_pc *C, u32 *cnk_count, u16 chunk_id, u16 max_cnk_cnt)
+{
+	long long m;
+        u16 b_popcnt = 0, c_popcnt = 0; 
+
+	if (chunk_id > (*cnk_count + 1) || chunk_id < 1) {
+		pr_err("Invalid chunk_id");
+		return -EINVAL;
+	}
+
+	if (*cnk_count >= max_cnk_cnt) {
+		pr_err("Cannot insert a new chunk. The chunk array is full");
+		return -EINVAL;
+	}
+
+	/*shift each element one step right to make
+	 *space for the new one
+	 */
+        memmove(&B[chunk_id * NUM_ELEMS_PER_CHUNK], 
+                &B[(chunk_id - 1) * NUM_ELEMS_PER_CHUNK], 
+                (*cnk_count - chunk_id + 1) * NUM_ELEMS_PER_CHUNK);
+        if (C) {
+            memmove(&C[chunk_id * NUM_ELEMS_PER_CHUNK], 
+                    &C[(chunk_id - 1) * NUM_ELEMS_PER_CHUNK], 
+                    (*cnk_count - chunk_id + 1) * NUM_ELEMS_PER_CHUNK);
+        }
+
+        /*Find the popcnt of the last element to the left*/
+        if (chunk_id > 1) {
+            b_popcnt = B[(chunk_id - 1) * NUM_ELEMS_PER_CHUNK - 1].popcnt;
+            if (C)
+                c_popcnt = C[(chunk_id - 1) * NUM_ELEMS_PER_CHUNK - 1].popcnt;
+        }
+            
+	/*Reset the newly created empty chunk*/
+	m = (chunk_id - 1) * NUM_ELEMS_PER_CHUNK;
+	for (; m < chunk_id * NUM_ELEMS_PER_CHUNK; m++) {
+            B[m].bitmap = 0;
+            B[m].popcnt = b_popcnt;
+            if (C) {
+                C[m].bitmap = 0;
+                C[m].popcnt = c_popcnt;
+            }
+	}
+
+	++(*cnk_count);
+	return 0;
+}
+
+static int chunk_insert(u8 *N, u8 *P, struct bitmap_pc *C, u32 *cnk_count, u16 chunk_id, u32 cnk_size)
+{
+	long long m;
+        u16 c_popcnt = 0; 
+        int elems_per_chunk_c_arr = cnk_size / 64;
+
+	if (chunk_id > (*cnk_count + 1) || chunk_id < 1) {
+		pr_err("Invalid chunk_id");
+		return -EINVAL;
+	}
+
+	if (*cnk_count >= SM_CHUNKS) {
+		pr_err("Cannot insert a new chunk. The chunk array is full");
+		return -EINVAL;
+	}
+
+	/*shift each element one step right to make
+	 *space for the new one
+	 */      
+        memmove(&N[chunk_id * cnk_size], &N[(chunk_id - 1) * cnk_size], 
+                (*cnk_count - chunk_id + 1) * cnk_size);
+        memmove(&P[chunk_id * cnk_size], &P[(chunk_id - 1) * cnk_size], 
+                (*cnk_count - chunk_id + 1) * cnk_size);
+
+	/*Reset the newly created empty chunk*/
+	m = (chunk_id - 1) * cnk_size;
+	for (; m < chunk_id * cnk_size; m++) {
+		N[m] = 0;
+		P[m] = 0;
+	}
+        
+        if (!C)
+            goto finish;
+        
+	/*shift each element one step right to make
+	 *space for the new one
+	 */
+        memmove(&C[chunk_id * elems_per_chunk_c_arr], 
+                &C[(chunk_id - 1) * elems_per_chunk_c_arr], 
+                (*cnk_count - chunk_id + 1) * elems_per_chunk_c_arr);
+
+        /*Find the popcnt of the last element to the left*/
+        if (chunk_id > 1)
+            c_popcnt = C[(chunk_id - 1) * elems_per_chunk_c_arr - 1].popcnt;
+        
+	/*Reset the newly created empty chunk*/
+	m = (chunk_id - 1) * elems_per_chunk_c_arr;
+	for (; m < chunk_id * elems_per_chunk_c_arr; m++) {
+            C[m].bitmap = 0;
+            C[m].popcnt = c_popcnt;
+	}
+
+finish:
+	++(*cnk_count);
+	return 0;
+}
+
+/*Calculate the chunk ID for level 24 based on C16*/
+static u16 calc_ckid24_from_C16(u16 *c16, u32 c16_size, u16 idx16)
+{
+	long long i;
+
+	if (idx16 >= c16_size) {
+		pr_err("Index needs to be smaller than arr_size");
+		return 0;
+	}
+
+	/*Find the first chunk ID to the left and increment that by 1*/
+	for (i = (long long)idx16 - 1; i >= 0; i--) {
+		if (c16[i] > 0)
+			return c16[i] + 1;
+	}
+
+	/*If there is no chunk to the left, then this is the first chunk*/
+	return 1;
+}
+
+/*Update C16 based on the newly inserted chunk*/
+static int update_C16(u16 *c16, u32 c16_size,
+		      u16 idx16, u16 chunk_id)
+{
+	long long i;
+
+	if (idx16 >= c16_size) {
+		pr_err("Invalid index");
+		return -EINVAL;
+	}
+
+	c16[idx16] = chunk_id;
+
+	/* Increment chunk ID to the right */
+	for (i = idx16 + 1; i < c16_size; i++) {
+		if (c16[i] > 0)
+			c16[i]++;
+	}
+
+	return 0;
+}
+
+/*Calculate the chunk ID for upper level based on CK and C of lower level*/
+static u16 calc_cid(struct bitmap_pc *c, u16 idx, u64 idx_off)
+{
+	long long i;
+	long long index = 0;
+        
+	/*find the index to C24 where previous chunk ID would be found*/
+	if (c[idx].bitmap) {
+		index = c[idx].popcnt +
+		    POPCNT_OFF(c[idx].bitmap, idx_off) - 1;
+                if (index >= CNK_16 - 2)
+                    goto err;
+		if (index >= 0)
+			return index + 2;
+	}
+
+	for (i = (long long)idx - 1; i >= 0; i--) {
+            if (c[i].bitmap) {
+                    index = c[i].popcnt + POPCNT(c[i].bitmap) - 1;
+                    if (index >= CNK_16 - 2)
+                        goto err;
+                    return index + 2;
+            }
+	}
+
+	/*If there is no chunk to the left, then this is the first chunk*/
+	return 1;
+err:
+        pr_err("Invalid index");
+        return -EINVAL;
+}
+
+/*Update CK and C based on the newly inserted chunk*/
+static int update_c(struct bitmap_pc *c, u32 idx, u32 idx_off, u32 cnk_count)
+{
+	long long i;
+	u64 index = 0;
+
+	if (c[idx].bitmap & (1ULL << idx_off)) {
+		pr_err("Error: bitmap is already set");
+		return -EINVAL;
+	}
+
+	/*find the index where Chunk ID should be copied to*/
+	if (c[idx].bitmap) {
+		index = c[idx].popcnt + POPCNT_OFF(c[idx].bitmap, idx_off);
+                if (index >= CNK_16)
+                    goto err;
+		goto index_found;
+	}
+
+	/*Find a chunk to the left which is not empty*/
+	for (i = (long long)idx - 1; i >= 0; i--) {
+            if (c[i].bitmap) {
+                    index = c[i].popcnt + POPCNT(c[i].bitmap);
+                    if (index >= CNK_16)
+                        goto err;
+                    goto index_found;
+            }	
+        }
+
+index_found:
+
+	/*This is the first element of this chunk*/
+	if (!c[idx].bitmap)
+		c[idx].popcnt = (u16)index;
+
+	c[idx].bitmap |= (1ULL << idx_off);
+
+	/*Update offset of the chunks to the right*/
+	for (i = (long long)idx + 1; i < cnk_count; i++)
+            if (c[i].bitmap)
+                    c[i].popcnt++;
+
+	return 0;
+err:
+        pr_err("Invalid index");
+        return -EINVAL;
+}
+
+int ppc_insert(struct PPC *p, u64 key, u8 prefix_len, struct net_device *dev)
+{
+	int i, j;
+	u8 *n16, *p16, *n32, *p32, *n40, *p40, *n48, *p48, *n64, *p64;
+	u16 *c16;
+        struct bitmap_pc *b32, *c32, *c40, *c48, *b64;
+	u16 chunk_id = 0;
+        u32 n_idx;
+        u32 c_idx;
+        u32 idx, idx_sail;
+        u16 ck_idx;
+	u16 n16_idx;/*Index to N16, P16 and C16*/	     
+	u32 off;
+	u32 num_leafs;/*Number of leafs need to be inserted for this prefix*/
+	u8 netdev_index = get_netdev_index(p, dev);
+	int err = 0;
+        u64 bitmap;
+        u64 tmp;
+        u32 new_prefixes = 0;
+
+	spin_lock(&p->lock);
+
+	/* Default route */
+	if (prefix_len == 0) {
+		p->def_nh = netdev_index;
+		goto finish;
+	}
+
+	/* Preallocate all the arrays at once*/
+	if (!p->N16) {
+		n16 = kcalloc(LEVEL16_SIZE, sizeof(*n16), GFP_ATOMIC);
+		p16 = kcalloc(LEVEL16_SIZE, sizeof(*p16), GFP_ATOMIC);
+		c16 = kcalloc(LEVEL16_SIZE, sizeof(*c16), GFP_ATOMIC);
+                
+                n32 = kcalloc(ARR_SIZE, sizeof(*n32), GFP_ATOMIC);
+                p32 = kcalloc(ARR_SIZE, sizeof(*p32), GFP_ATOMIC);
+                b32 = kcalloc(BC_SIZE, sizeof(*b32), GFP_ATOMIC);
+                c32 = kcalloc(BC_SIZE, sizeof(*c32), GFP_ATOMIC);
+
+                n40 = kcalloc(ARR_SIZE_SM, sizeof(*n40), GFP_ATOMIC);
+                p40 = kcalloc(ARR_SIZE_SM, sizeof(*p40), GFP_ATOMIC);
+                c40 = kcalloc(BC_SIZE_SM, sizeof(*c40), GFP_ATOMIC);
+                
+                n48 = kcalloc(ARR_SIZE_SM, sizeof(*n48), GFP_ATOMIC);
+                p48 = kcalloc(ARR_SIZE_SM, sizeof(*p48), GFP_ATOMIC);
+                c48 = kcalloc(BC_SIZE_SM, sizeof(*c48), GFP_ATOMIC);
+
+                n64 = kcalloc(ARR_SIZE, sizeof(*n64), GFP_ATOMIC);
+                p64 = kcalloc(ARR_SIZE, sizeof(*p64), GFP_ATOMIC);
+                b64 = kcalloc(B64_SIZE, sizeof(*b64), GFP_ATOMIC);
+                
+		if (!n16 || !c16 || !p16 || !n32 || !p32 || !b32 || !c32
+                        || !n40 || !p40 || !c40 || 
+                        !n48 || !p48 || !c48 || !n64 || !p64 || !b64) {
+			kfree(n16);
+			kfree(c16);
+			kfree(p16);
+                        kfree(n32);
+                        kfree(b32);
+                        kfree(c32);
+                        kfree(p32);
+                        kfree(n40);
+                        kfree(p40);
+                        kfree(c40);
+                        kfree(n48);
+                        kfree(p48);
+                        kfree(c40);
+                        kfree(n64);
+                        kfree(p64);
+                        kfree(b64);
+			pr_err("Out of memory while preallocating  SAIL");
+			goto error;
+		}
+
+		RCU_INIT_POINTER(p->N16, n16);
+		RCU_INIT_POINTER(p->P16, p16);
+		RCU_INIT_POINTER(p->C16, c16);
+
+                RCU_INIT_POINTER(p->N32, n32);
+                RCU_INIT_POINTER(p->P32, p32);
+                RCU_INIT_POINTER(p->B32, b32);
+                RCU_INIT_POINTER(p->C32, c32);
+                p->n32_cnt = 0;
+
+                RCU_INIT_POINTER(p->N40, n40);
+                RCU_INIT_POINTER(p->P40, p40);
+                RCU_INIT_POINTER(p->C40, c40);
+                
+                RCU_INIT_POINTER(p->N48, n48);
+                RCU_INIT_POINTER(p->P48, p48);
+                RCU_INIT_POINTER(p->C48, c48);
+
+                RCU_INIT_POINTER(p->N64, n64);
+                RCU_INIT_POINTER(p->P64, p64);
+                RCU_INIT_POINTER(p->B64, b64);
+                p->n64_cnt = 0;
+                
+//		synchronize_rcu();
+	}
+
+	/*Eextract 16 bits from MSB.*/
+	n16_idx = key >> 48;
+
+	if (prefix_len <= 16) {
+		/*All the leafs in level 1~16 will be stored in level 16.*/
+		num_leafs = 1U << (16 - prefix_len);
+		for (i = 0; i < num_leafs; i++) {
+			/*Longer prefix exists*/
+			if (p->P16[n16_idx + i] > prefix_len)
+				continue;
+			p->N16[n16_idx + i] = netdev_index;
+			p->P16[n16_idx + i] = prefix_len;
+		}
+		goto finish;
+	}
+
+	/* The length of the prefix is 21~64. So need to check if there is a
+	 * chunk for this prefix in level 24. C16[..] = 0 indicates that there
+	 * is no chunk, so need to insert one. The insertion works as
+	 * following:
+	 * 1. Calculate the chunk ID to level 24 from C16. The Chunk ID
+	 * indicates where the new chunk should be inserted.
+	 * 2. Insert a new chunk in level 24
+	 * 3. Update the C16[] based on the newly inserted chunk
+	 */
+	if (p->C16[n16_idx] == 0) {
+		/*Step 1*/
+		chunk_id = calc_ckid24_from_C16(p->C16, LEVEL16_SIZE, n16_idx);
+		if (!chunk_id)
+			goto error;
+		/*Step 2*/
+		err = bc_arr_insert(p->B32, p->C32, &p->cnk32_count, chunk_id, NUM_CHUNKS);
+		if (err) {
+                        pr_err("Could not insert chunk to level 32");
+			goto error;
+                }
+		/*Step 3*/
+		err = update_C16(p->C16, LEVEL16_SIZE, n16_idx, chunk_id);
+		if (err)
+			goto error;
+	}
+
+	/*Extract bit 17~32 from MSB and calculate index to B32/C32*/
+        idx_sail = (p->C16[n16_idx] - 1) * CNK_16 + ((key >> 32) & 0XFFFF);
+        idx = idx_sail / 64;
+        /*bit offset in a 64-bit bitmap*/
+        off = idx_sail % 64;
+        
+	if (prefix_len <= 32) {
+		/*All the leafs in level 17~32 will be stored in level 32.*/
+		num_leafs = 1U << (32 - prefix_len);
+		for (i = 0, tmp = 1ULL << off; i < num_leafs; i++, tmp <<= 1) {
+                    n_idx = calc_idx(&(p->B32[idx]), off + i);
+                    if (p->B32[idx].bitmap & tmp) {                        
+			/*Longer prefix exists*/
+                        if (p->P32[n_idx] > prefix_len)
+                                continue;
+                        p->N32[n_idx] = netdev_index;
+                        p->P32[n_idx] = prefix_len;
+                    } else {
+                        if (n_idx >= ARR_SIZE)
+                            goto error;
+                        /*FIXME: If this loop goes for long, that breaks the kerenel*/
+                        for (j = (int)p->n32_cnt - 1; j >= (int)n_idx; j--) {
+                                p->N32[j + 1] = p->N32[j];
+                                p->P32[j + 1] = p->P32[j];
+                        }
+                        p->N32[n_idx] = netdev_index;
+                        p->P32[n_idx] = prefix_len;
+                        new_prefixes++;
+                        p->n32_cnt++;
+                    }
+		}
+
+                for (i = idx + 1; i < NUM_ELEMS_PER_CHUNK * p->cnk32_count; i++) {
+                    p->B32[i].popcnt += new_prefixes;
+                }
+
+                bitmap = ((1ULL << num_leafs) - 1) << off;
+                p->B32[idx].bitmap |= bitmap;
+		goto finish;
+	}
+
+
+        /* The length of the prefix is 33-64, but there is no chunk for
+         * this prefix in level 40. So do the same as before
+         */
+
+        if (!(p->C32[idx].bitmap & (1ULL << off))) {
+                chunk_id = calc_cid(p->C32, idx, off);
+                if (!chunk_id)
+                        goto error;
+                err = chunk_insert(p->N40, p->P40, p->C40, &p->cnk40_count, chunk_id, CNK_8);
+		if (err) {
+                        pr_err("Could not insert chunk to level 40");
+			goto error;
+                }
+                err = update_c(p->C32, idx, off, p->cnk32_count);
+                if (err)
+                        goto error;
+        }
+
+        ck_idx = calc_idx(&(p->C32[idx]), off);
+        /////// start from here
+        /*Extract 16 more bits and calculate index to level 48, 64,..*/
+        n_idx = (ck_idx * CNK_8) + ((key >> 24) & 0XFF);
+        c_idx = n_idx / 64; 
+        off = n_idx % 64;
+
+        if (prefix_len <= 40) {
+                /*All the leafs in level 33~48 will be stored in level 48.*/
+                num_leafs = 1U << (40 - prefix_len);
+                for (i = 0; i < num_leafs; i++) {
+                        /*Longer prefix exists*/
+                        if (p->P40[n_idx + i] > prefix_len)
+                                continue;
+                        p->N40[n_idx + i] = netdev_index;
+                        p->P40[n_idx + i] = prefix_len;
+                }
+                goto finish;
+        }
+        
+        /* The length of the prefix is 33-64, but there is no chunk for
+         * this prefix in the next level. So do the same as before
+         */
+        if (!(p->C40[c_idx].bitmap & (1ULL << off))) {
+                chunk_id = calc_cid(p->C40, c_idx, off);
+                if (!chunk_id)
+                        goto error;
+                err = chunk_insert(p->N48, p->P48, p->C48, &p->cnk48_count, chunk_id, CNK_8);
+		if (err) {
+                        pr_err("Could not insert chunk to level 48");
+			goto error;
+                }
+                err = update_c(p->C40, c_idx, off, p->cnk40_count);
+                if (err)
+                        goto error;
+        }
+        
+        ck_idx = calc_idx(&(p->C40[c_idx]), off);
+        /*Extract 16 more bits and calculate index to level 48, 64,..*/
+        n_idx = (ck_idx * CNK_8) + ((key >> 16) & 0XFF);
+        c_idx = n_idx / 64; 
+        off = n_idx % 64;
+
+        if (prefix_len <= 48) {
+                /*All the leafs in level 33~48 will be stored in level 48.*/
+                num_leafs = 1U << (48 - prefix_len);
+                for (i = 0; i < num_leafs; i++) {
+                        /*Longer prefix exists*/
+                        if (p->P48[n_idx + i] > prefix_len)
+                                continue;
+                        p->N48[n_idx + i] = netdev_index;
+                        p->P48[n_idx + i] = prefix_len;
+                }
+                goto finish;
+        }
+        
+        /* The length of the prefix is 49-64, but there is no chunk for
+         * this prefix in the next level. So do the same as before
+         */
+        if (!(p->C48[c_idx].bitmap & (1ULL << off))) {
+                chunk_id = calc_cid(p->C48, c_idx, off);
+                if (!chunk_id)
+                        goto error;
+                err = bc_arr_insert(p->B64, NULL, &p->cnk64_count, chunk_id, NUM_CHUNKS_LAST);
+		if (err) {
+                        pr_err("Could not insert chunk to level 64");
+			goto error;
+                }
+                err = update_c(p->C48, c_idx, off, p->cnk48_count);
+                if (err)
+                        goto error;
+        }
+
+        /*Extract 16 more bits and calculate index to level 48, 64,..*/
+        ck_idx = calc_idx(&(p->C48[c_idx]), off);
+        idx_sail = (ck_idx * CNK_16) + (key & 0XFFFF);        
+        idx = idx_sail / 64;
+        off = idx_sail % 64;
+
+        if (prefix_len <= 64) {
+		/*All the leafs in level 17~32 will be stored in level 32.*/
+		num_leafs = 1U << (64 - prefix_len);
+		for (i = 0, tmp = 1ULL << off; i < num_leafs; i++, tmp <<= 1) {
+                    n_idx = calc_idx(&(p->B64[idx]), off + i);
+                    if (p->B64[idx].bitmap & tmp) {                        
+			/*Longer prefix exists*/
+                        if (p->P64[n_idx] > prefix_len)
+                                continue;
+                        p->N64[n_idx] = netdev_index;
+                        p->P64[n_idx] = prefix_len;
+                    } else {
+                        if (n_idx >= ARR_SIZE)
+                            goto error;
+                        /*FIXME: If this loop goes for long, that breaks the kerenel*/
+                        for (j = (int)p->n64_cnt - 1; j >= (int)n_idx; j--) {
+                                p->N64[j + 1] = p->N64[j];
+                                p->P64[j + 1] = p->P64[j];
+                        }
+                        p->N64[n_idx] = netdev_index;
+                        p->P64[n_idx] = prefix_len;
+                        new_prefixes++;
+                        p->n64_cnt++;
+                    }
+		}
+                
+                for (i = idx + 1; i < NUM_ELEMS_PER_CHUNK * p->cnk64_count; i++)
+                    p->B64[i].popcnt += new_prefixes;
+                
+                bitmap = ((1ULL << num_leafs) - 1) << off;
+                p->B64[idx].bitmap |= bitmap;
+		goto finish;
+        }       
+
+error:
+	pr_err("Something went wrong in route insertion");
+finish:
+	spin_unlock(&p->lock);
+	return err;
+}
+
+int ppc_lookup(const struct PPC *p, const u64 key, struct net_device **dev)
+{
+	u8 *n16, *n;
+	u16 *c16;
+        struct bitmap_pc *b, *c;
+        u32 off;
+	u8 netdev_index = p->def_nh;
+	u16 n16_idx;/*Index to N16 and C16*/
+        u32 idx, idx_sail;/*Index to B/C*/
+	u32 n_idx;/*Index to N*/
+	u32 c_idx;/*Index to C*/
+	u32 ck_idx;/*Index to CK*/
+	u64 ck_off;/*Index to CK*/
+
+	rcu_read_lock();
+
+	/*extract 16 bits from MSB*/
+	n16_idx = key >> 48;
+	n16 = rcu_dereference(p->N16);
+	if (unlikely(!n16))
+		goto finish;
+
+        /*Find corresponding next-hop in level 16*/
+	if (likely(n16[n16_idx] != 0))
+		netdev_index = n16[n16_idx];
+
+	/*Check if there is a longer prefix; if yes, extract bit 17~32
+	 *  and calculate index to N32
+	 */
+	c16 = rcu_dereference(p->C16);
+	if (likely(c16[n16_idx] != 0)) {
+		idx_sail = (c16[n16_idx] - 1) * CNK_16 + ((key >> 32) & 0XFFFF);
+                idx = idx_sail / 64;
+                off = idx_sail % 64;
+	} else {
+		goto finish;
+	}
+
+        /*Find corresponding next-hop in level 32*/
+        c = rcu_dereference(p->C32);
+        b = rcu_dereference(p->B32);
+        if (b[idx].bitmap & (1ULL << off)) {
+            n = rcu_dereference(p->N32);
+            n_idx = calc_idx(&(b[idx]), off);
+            netdev_index = n[n_idx];      
+        }
+
+        /*Check if there is a longer prefix; if yes, extract 33-40 bit
+         * and calculate index to N40
+         */
+        if (likely(c[idx].bitmap & (1ULL << off))) {
+                ck_idx = c[idx].popcnt + POPCNT_OFF(c[idx].bitmap, off);
+                ck_off = (key >> 24) & 0XFF;
+                n_idx = ck_idx * CNK_8 + ck_off;
+                c_idx = n_idx / 64;
+                off = n_idx % 64;
+        } else {
+                goto finish;
+        }
+
+        /*Find corresponding next-hop in level 40*/
+        n = rcu_dereference(p->N40);
+        if (likely(n[n_idx] != 0))
+                netdev_index = n[n_idx];
+        
+        /*Check if there is a longer prefix; if yes, extract 41-48 bit
+         * and calculate index to N48
+         */
+        c = rcu_dereference(p->C40);
+        if (likely(c[c_idx].bitmap & (1ULL << off))) {
+                ck_idx = c[c_idx].popcnt + POPCNT_OFF(c[c_idx].bitmap, off);
+                ck_off = (key >> 16) & 0XFF;
+                n_idx = ck_idx * CNK_8 + ck_off;
+                c_idx = n_idx / 64;
+                off = n_idx % 64;
+        } else {
+                goto finish;
+        }
+
+        /*Find corresponding next-hop in level 48*/
+        n = rcu_dereference(p->N48);
+        if (likely(n[n_idx] != 0))
+                netdev_index = n[n_idx];
+        
+        /*Check if there is a longer prefix; if yes, 49-64 bit
+         * and calculate index to N64
+         */
+        c = rcu_dereference(p->C48);
+        if (likely(c[c_idx].bitmap & (1ULL << off))) {
+                ck_idx = c[c_idx].popcnt + POPCNT_OFF(c[c_idx].bitmap, off);
+                idx_sail = ck_idx * CNK_16 + (key & 0XFFFF);
+                idx = idx_sail / 64;
+                off = idx_sail % 64;
+        } else {
+                goto finish;
+        }
+
+        /*Find corresponding next-hop in level 64*/
+        b = rcu_dereference(p->B64);
+        if (b[idx].bitmap & (1ULL << off)) {
+            n = rcu_dereference(p->N64);
+            n_idx = calc_idx(&(b[idx]), off);
+            netdev_index = n[n_idx];      
+        }
+
+finish:
+	*dev = p->netdevs[netdev_index];
+	rcu_read_unlock();
+	return 0;
+}
