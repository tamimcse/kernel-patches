+
+#include <net/ip6_fib.h>
+
+/*The length of N16, P16 and C16 is 2^16*/
+#define LEVEL16_SIZE 65536
+
+/*Length of C array in level 32, 64.*/
+#define C_SIZE 6000
+
+/*Length of C array for level 40 and 48*/
+#define C_SIZE_SMALL 65536 
+
+/*Number of small chunks*/
+#define SM_CHUNKS 16384
+
+/*chunk size is 2^16*/
+#define CNK_16 65536
+
+/*chunk size is 2^8*/
+#define CNK_8 256
+
+/*Total number of chunks preallocated for level 24-64*/
+#define NUM_CHUNKS 64
+
+/*memory preallocated for N and P array in level 24-64*/
+#define ARR_SIZE NUM_CHUNKS * CNK_16
+
+#define ARR_SIZE_SM SM_CHUNKS * CNK_8
+
+/*Calculates the number of bits set to 1*/
+#define POPCNT(X) (hweight64(X))
+
+/*POPCNT of left-most N bits of X*/
+#define POPCNT_OFF(X, N) (hweight64(((1ULL << (N)) - 1) & (X)))
+
+/*Calculate index to C24 from CK26 chunk and chunk offset */
+static u64 calc_c_idx(struct chunk *c, u64 cnk_off)
+{
+	u32 part_idx, part_off;
+
+	part_idx = cnk_off / 64;
+	part_off = cnk_off % 64;
+
+	return c->start_index[part_idx] +
+		POPCNT_OFF(c->bitmap[part_idx], part_off);
+}
+
+static u64 calc_c_idx_sm(struct chunk_small *c, u64 cnk_off)
+{
+	u8 part_idx, part_off;
+
+	part_idx = cnk_off / 64;
+	part_off = cnk_off % 64;
+
+	return c->start_index[part_idx] +
+		POPCNT_OFF(c->bitmap[part_idx], part_off);
+}
+
+/*Converts a net_device to corresponding netdev_index*/
+static u8 get_netdev_index(struct sail *s, struct net_device *dev)
+{
+	u8 i;
+
+	/*checks if the net_device is already seen; if yes then return the
+	 *corresponding index
+	 */
+	for (i = 0; i < s->netdev_count; i++) {
+		if (s->netdevs[i] == dev)
+			return i;
+	}
+	/*If the net_device is not previously seen, then add it to the array*/
+	s->netdevs[s->netdev_count++] = dev;
+	return s->netdev_count - 1;
+}
+
+/* Insert a new chunk to a level at chunk_id-1. Note that Chunk ID
+ * starts from 1, not 0. This function increments cnk_count of sail.
+ * This is why, it is passed by reference
+ */
+static int chunk_insert(u8 *N, u8 *P, struct chunk *ck, u32 *cnk_count, u16 chunk_id, u32 cnk_size)
+{
+	long long m;
+
+	if (chunk_id > (*cnk_count + 1) || chunk_id < 1) {
+		pr_err("Invalid chunk_id");
+		return -EINVAL;
+	}
+
+	if (*cnk_count >= NUM_CHUNKS) {
+		pr_err("Cannot insert a new chunk. The chunk array is full");
+		return -EINVAL;
+	}
+
+	/*shift each element one step right to make
+	 *space for the new one
+	 */
+	m = (long long)(*cnk_count) * cnk_size - 1;
+	for (; m >= (chunk_id - 1) * cnk_size; m--) {
+		N[m + cnk_size] = N[m];
+		P[m + cnk_size] = P[m];
+	}
+
+	/*Reset the newly created empty chunk*/
+	m = (chunk_id - 1) * cnk_size;
+	for (; m < chunk_id * cnk_size; m++) {
+		N[m] = 0;
+		P[m] = 0;
+	}
+        
+        if (!ck)
+            goto finish;
+        
+        /*shift each chunk one step right to make
+         *space for the new one
+         */
+        m = (long long)(*cnk_count) - 1;
+        for (; m >= (chunk_id - 1); m--)
+                ck[m + 1] = ck[m];
+
+        /*Reset the newly created empty chunk*/
+        for (m = 0; m < PARTS; m++) {
+                ck[chunk_id - 1].bitmap[m] = 0;
+                ck[chunk_id - 1].start_index[m] = 0;
+        }
+
+finish:
+	++(*cnk_count);
+	return 0;
+}
+
+static int chunk_sm_insert(u8 *N, u8 *P, struct chunk_small *ck, u32 *cnk_count, u16 chunk_id, u32 cnk_size)
+{
+	long long m;
+
+	if (chunk_id > (*cnk_count + 1) || chunk_id < 1) {
+		pr_err("Invalid chunk_id");
+		return -EINVAL;
+	}
+
+	if (*cnk_count >= SM_CHUNKS) {
+		pr_err("Cannot insert a new chunk. The chunk array is full");
+		return -EINVAL;
+	}
+
+	/*shift each element one step right to make
+	 *space for the new one
+	 */
+	m = (long long)(*cnk_count) * cnk_size - 1;
+	for (; m >= (chunk_id - 1) * cnk_size; m--) {
+		N[m + cnk_size] = N[m];
+		P[m + cnk_size] = P[m];
+	}
+
+	/*Reset the newly created empty chunk*/
+	m = (chunk_id - 1) * cnk_size;
+	for (; m < chunk_id * cnk_size; m++) {
+		N[m] = 0;
+		P[m] = 0;
+	}
+        
+        if (!ck)
+            goto finish;
+        
+            /*shift each chunk one step right to make
+             *space for the new one
+             */
+            m = (long long)(*cnk_count) - 1;
+            for (; m >= (chunk_id - 1); m--)
+                    ck[m + 1] = ck[m];
+
+            /*Reset the newly created empty chunk*/
+            for (m = 0; m < SMALL; m++) {
+                    ck[chunk_id - 1].bitmap[m] = 0;
+                    ck[chunk_id - 1].start_index[m] = 0;
+            }
+        
+
+finish:
+	++(*cnk_count);
+	return 0;
+}
+
+static int chunk_delete(u8 *N, u8 *P, struct chunk *CK, u32 *cnk_count, u16 chunk_id, u32 cnk_size)
+{
+	long long m;
+	u64 end_idx;
+
+	if (chunk_id > *cnk_count) {
+		pr_err("Invalid chunk_id");
+		return -EINVAL;
+	}
+
+	if (chunk_id < *cnk_count) {
+		/*shift each chunk one step left*/
+		m = (long long)chunk_id * cnk_size;
+		end_idx = (*cnk_count) * cnk_size - 1;
+		for (; m <= end_idx; m++) {
+			N[m - cnk_size] = N[m];
+			P[m - cnk_size] = P[m];
+		}
+	}
+
+	/*Reset the the last chunk*/
+	end_idx = (*cnk_count) * cnk_size - 1;
+	m = (*cnk_count - 1) * cnk_size;
+	for (; m <= end_idx; m++) {
+		N[m] = 0;
+		P[m] = 0;
+	}
+
+	if (chunk_id < *cnk_count) {
+		/*shift each chunk one step left*/
+		m = (long long)chunk_id;
+		for (; m <= *cnk_count - 1; m++)
+			CK[m - 1] = CK[m];
+	}
+
+	/*Reset the the last chunk*/
+	for (m = 0; m < PARTS; m++) {
+		CK[*cnk_count - 1].start_index[m] = 0;
+		CK[*cnk_count - 1].bitmap[m] = 0;
+	}
+        *cnk_count -= 1;
+	return 0;
+}
+
+/*Calculate the chunk ID for level 24 based on C16*/
+static u16 calc_ckid24_from_C16(u16 *c16, u32 c16_size, u16 idx16)
+{
+	long long i;
+
+	if (idx16 >= c16_size) {
+		pr_err("Index needs to be smaller than arr_size");
+		return 0;
+	}
+
+	/*Find the first chunk ID to the left and increment that by 1*/
+	for (i = (long long)idx16 - 1; i >= 0; i--) {
+		if (c16[i] > 0)
+			return c16[i] + 1;
+	}
+
+	/*If there is no chunk to the left, then this is the first chunk*/
+	return 1;
+}
+
+/*Update C16 based on the newly inserted chunk*/
+static int update_C16(u16 *c16, u32 c16_size,
+		      u16 idx16, u16 chunk_id)
+{
+	long long i;
+
+	if (idx16 >= c16_size) {
+		pr_err("Invalid index");
+		return -EINVAL;
+	}
+
+	c16[idx16] = chunk_id;
+
+	/* Increment chunk ID to the right */
+	for (i = idx16 + 1; i < c16_size; i++) {
+		if (c16[i] > 0)
+			c16[i]++;
+	}
+
+	return 0;
+}
+
+/*Remove Chunk ID for level 16*/
+static int C16_remove_chunkid(u16 *c16, u32 c16_size, u16 chunk_id)
+{
+	long long i;
+	bool found = false;
+
+	/*Check if the chunk ID already exists*/
+	for (i = 0; i < c16_size; i++) {
+		if (found && c16[i] > 0) {
+			c16[i]--;
+		} else if (c16[i] == chunk_id) {
+			c16[i] = 0;
+			found = true;
+		}
+	}
+
+	return 0;
+}
+
+/*This function removes the chunk ID of the lower level after deleting
+ * a chunk from upper level
+ */
+static int remove_chunkid(u16 *C, struct chunk *CK, u16 cnk_idx, u64 cnk_off)
+{
+	long long i;
+	bool found = false;
+	u32 chunk_id;
+	u8 part_idx, part_off;
+
+	part_idx = cnk_off / 64;
+	part_off = cnk_off % 64;
+
+	chunk_id = C[calc_c_idx(&CK[cnk_idx], cnk_off)];
+	CK[cnk_idx].bitmap[part_idx] &= ~(1ULL << part_off);
+
+	for (i = 0; i < C_SIZE; i++) {
+		if (C[i] == chunk_id)
+			found = true;
+		else if (found && C[i] > 0)
+			C[i - 1] = C[i];
+		else if (found && !C[i])
+			break;
+	}
+
+	return 0;
+}
+
+/* Check if a chunk is unused*/
+static bool is_chunk_unused(u8 *nh, struct chunk *ck, u16 chunk_id, u32 cnk_size)
+{
+	long long i;
+	long long start_index, end_index;
+
+	if (chunk_id < 1) {
+		pr_err("Invalid chunk ID");
+		return -EINVAL;
+	}
+
+	start_index = (long long)(chunk_id - 1) * cnk_size;
+	end_index = (long long)chunk_id * cnk_size - 1;
+	for (i = start_index; i <= end_index; i++) {
+		/*The chunk is being used*/
+		if (nh[i] > 0)
+			return false;
+	}
+
+        for ( i = 0; i < PARTS; i++)
+            if (ck->bitmap[i])
+		return false;
+
+	return true;
+}
+
+/*Calculate the chunk ID for upper level based on CK and C of lower level*/
+static u16 calc_ckid(struct chunk *ck, u16 ck_idx, u64 ck_off, u16 *c_arr)
+{
+	long long i, j;
+	long long index = 0;
+	u32 part_off, part_idx;
+//	struct chunk *c;
+
+	part_idx = ck_off / 64;
+	part_off = ck_off % 64;
+//	c = &ck[ck_idx];
+        
+	/*find the index to C24 where previous chunk ID would be found*/
+	if (ck[ck_idx].bitmap[part_idx]) {
+		index = ck[ck_idx].start_index[part_idx] +
+		    POPCNT_OFF(ck[ck_idx].bitmap[part_idx], part_off) - 1;
+		if (index >= 0)
+			goto index_found;
+	}
+
+	j = (long long)part_idx - 1;
+	for (i = (long long)ck_idx; i >= 0; i--) {
+		for (; j >= 0; j--) {
+			if (ck[i].bitmap[j]) {
+				index = ck[i].start_index[j] +
+					POPCNT(ck[i].bitmap[j]) - 1;
+				goto index_found;
+			}
+		}
+		j = PARTS - 1;
+	}
+
+	/*If there is no chunk to the left, then this is the first chunk*/
+	return 1;
+index_found:
+	if (index >= (C_SIZE - 1)) {
+		pr_err("C array is full. Cannot insert");
+		return 0;
+	} else if (c_arr[index] <= 0) {
+		pr_err("Invalid chunk ID");
+		return 0;
+	} else {
+		return c_arr[index] + 1;
+	}
+}
+
+/*Calculate the chunk ID for upper level based on CK and C of lower level*/
+static u16 calc_ckid_sm(struct chunk_small *ck, u16 ck_idx, u64 ck_off, u16 *c_arr)
+{
+	long long i, j;
+	long long index = 0;
+	u8 part_off, part_idx;
+//	struct chunk *c;
+
+	part_idx = ck_off / 64;
+	part_off = ck_off % 64;
+//	c = &ck[ck_idx];
+
+	/*find the index to C24 where previous chunk ID would be found*/
+	if (ck[ck_idx].bitmap[part_idx]) {
+		index = ck[ck_idx].start_index[part_idx] +
+		    POPCNT_OFF(ck[ck_idx].bitmap[part_idx], part_off) - 1;
+		if (index >= 0)
+			goto index_found;
+	}
+
+	j = (long long)part_idx - 1;
+	for (i = (long long)ck_idx; i >= 0; i--) {
+		for (; j >= 0; j--) {
+			if (ck[i].bitmap[j]) {
+				index = ck[i].start_index[j] +
+					POPCNT(ck[i].bitmap[j]) - 1;
+				goto index_found;
+			}
+		}
+		j = SMALL - 1;
+	}
+
+	/*If there is no chunk to the left, then this is the first chunk*/
+	return 1;
+index_found:
+	if (index >= (C_SIZE_SMALL - 1)) {
+		pr_err("C array is full. Cannot insert");
+		return 0;
+	} else if (c_arr[index] <= 0) {
+		pr_err("Invalid chunk ID");
+		return 0;
+	} else {
+		return c_arr[index] + 1;
+	}
+}
+
+/*Update CK and C based on the newly inserted chunk*/
+static int update_ck_c(struct chunk *ck, u32 ck_idx, u32 ck_off,
+			   u16 *c, u32 cnk_count, u16 chunk_id)
+{
+	long long i, j;
+	u64 index = 0;
+	u32 part_idx, part_off;
+
+	part_idx = ck_off / 64;
+	part_off = ck_off % 64;
+
+	if (ck[ck_idx].bitmap[part_idx] & (1ULL << part_off)) {
+		pr_err("Error: bitmap is already set");
+		return -EINVAL;
+	}
+
+	/*find the index where Chunk ID should be copied to*/
+	if (ck[ck_idx].bitmap[part_idx]) {
+		index = ck[ck_idx].start_index[part_idx] +
+			POPCNT_OFF(ck[ck_idx].bitmap[part_idx], part_off);
+		goto index_found;
+	}
+
+	/*Find a chunk to the left which is not empty*/
+	j = (long long)part_idx - 1;
+	for (i = (long long)ck_idx; i >= 0; i--) {
+		for (; j >= 0; j--) {
+			if (ck[i].bitmap[j]) {
+				index = ck[i].start_index[j] +
+					POPCNT(ck[i].bitmap[j]);
+				goto index_found;
+			}
+		}
+		j = PARTS - 1;
+	}
+
+index_found:
+	/* Move each element one step to the right to create a space for
+	 * the new element. Also increment each element by 1
+	 */
+	for (i = C_SIZE - 2 ; i >= (long long)index; i--)
+		c[i + 1] = (c[i] > 0) ? c[i] + 1 : c[i];
+	/*Set the new Chunk ID*/
+	c[index] = chunk_id;
+
+	/*This is the first element of this chunk*/
+	if (!ck[ck_idx].bitmap[part_idx])
+		ck[ck_idx].start_index[part_idx] = index;
+
+	ck[ck_idx].bitmap[part_idx] |= (1ULL << part_off);
+
+	/*Update offset of the chunks to the right*/
+	j = part_idx + 1;
+	for (i = (long long)ck_idx; i < cnk_count; i++) {
+		for (; j < PARTS; j++) {
+			if (ck[i].bitmap[j])
+				ck[i].start_index[j]++;
+		}
+		j = 0;
+	}
+
+	return 0;
+}
+
+/*Update CK and C based on the newly inserted chunk*/
+static int update_ck_sm_c(struct chunk_small *ck, u32 ck_idx, u32 ck_off,
+			   u16 *c, u32 cnk_count, u16 chunk_id)
+{
+	long long i, j;
+	u64 index = 0;
+	u8 part_idx, part_off;
+
+	part_idx = ck_off / 64;
+	part_off = ck_off % 64;
+
+	if (ck[ck_idx].bitmap[part_idx] & (1ULL << part_off)) {
+		pr_err("Error: bitmap is already set");
+		return -EINVAL;
+	}
+
+	/*find the index where Chunk ID should be copied to*/
+	if (ck[ck_idx].bitmap[part_idx]) {
+		index = ck[ck_idx].start_index[part_idx] +
+			POPCNT_OFF(ck[ck_idx].bitmap[part_idx], part_off);
+		goto index_found;
+	}
+
+	/*Find a chunk to the left which is not empty*/
+	j = (long long)part_idx - 1;
+	for (i = (long long)ck_idx; i >= 0; i--) {
+		for (; j >= 0; j--) {
+			if (ck[i].bitmap[j]) {
+				index = ck[i].start_index[j] +
+					POPCNT(ck[i].bitmap[j]);
+				goto index_found;
+			}
+		}
+		j = SMALL - 1;
+	}
+
+index_found:
+	/* Move each element one step to the right to create a space for
+	 * the new element. Also increment each element by 1
+	 */
+	for (i = C_SIZE_SMALL - 2 ; i >= (long long)index; i--)
+		c[i + 1] = (c[i] > 0) ? c[i] + 1 : c[i];
+	/*Set the new Chunk ID*/
+	c[index] = chunk_id;
+
+	/*This is the first element of this chunk*/
+	if (!ck[ck_idx].bitmap[part_idx])
+		ck[ck_idx].start_index[part_idx] = index;
+
+	ck[ck_idx].bitmap[part_idx] |= (1ULL << part_off);
+
+	/*Update offset of the chunks to the right*/
+	j = part_idx + 1;
+	for (i = (long long)ck_idx; i < cnk_count; i++) {
+		for (; j < SMALL; j++) {
+			if (ck[i].bitmap[j])
+				ck[i].start_index[j]++;
+		}
+		j = 0;
+	}
+
+	return 0;
+}
+
+int sail_insert(struct sail *s, u64 key, u8 prefix_len, struct net_device *dev)
+{
+	int i, j;
+        u8 k;
+        u8 bitshifts;
+	u8 *n16, *p16, *n, *p, *n32, *p32, *n40, *p40, *n48, *p48, *n64, *p64;
+	u16 *c16, *c, *c32, *c40, *c48;
+	struct chunk *ck, *ck32;
+        struct chunk_small *ck40, *ck48;
+	u16 chunk_id;
+        u32 n_idx;
+	u64 c_idx;
+        u16 ck_idx;
+        u64 ck_off;
+	u16 n16_idx;/*Index to N16, P16 and C16*/	     
+	u32 part_idx, part_off;
+	u32 num_leafs;/*Number of leafs need to be inserted for this prefix*/
+	u8 netdev_index = get_netdev_index(s, dev);
+	int err = 0;
+
+	spin_lock(&s->lock);
+
+	/* Default route */
+	if (prefix_len == 0) {
+		s->def_nh = netdev_index;
+		goto finish;
+	}
+
+	/* Preallocate all the arrays at once*/
+	if (!s->N16) {
+		n16 = kcalloc(LEVEL16_SIZE, sizeof(*n16), GFP_ATOMIC);
+		p16 = kcalloc(LEVEL16_SIZE, sizeof(*p16), GFP_ATOMIC);
+		c16 = kcalloc(LEVEL16_SIZE, sizeof(*c16), GFP_ATOMIC);
+                
+                n32 = kcalloc(ARR_SIZE, sizeof(*n32), GFP_ATOMIC);
+                p32 = kcalloc(ARR_SIZE, sizeof(*p32), GFP_ATOMIC);
+                ck32 = kcalloc(NUM_CHUNKS, sizeof(*ck32), GFP_ATOMIC);
+                c32 = kcalloc(C_SIZE, sizeof(*c32), GFP_ATOMIC);
+
+                n40 = kcalloc(ARR_SIZE_SM, sizeof(*n40), GFP_ATOMIC);
+                p40 = kcalloc(ARR_SIZE_SM, sizeof(*p40), GFP_ATOMIC);
+                ck40 = kcalloc(SM_CHUNKS, sizeof(*ck40), GFP_ATOMIC);
+                c40 = kcalloc(C_SIZE_SMALL, sizeof(*c40), GFP_ATOMIC);
+                
+                n48 = kcalloc(ARR_SIZE_SM, sizeof(*n48), GFP_ATOMIC);
+                p48 = kcalloc(ARR_SIZE_SM, sizeof(*p48), GFP_ATOMIC);
+                ck48 = kcalloc(SM_CHUNKS, sizeof(*ck48), GFP_ATOMIC);
+                c48 = kcalloc(C_SIZE_SMALL, sizeof(*c48), GFP_ATOMIC);
+
+                n64 = kcalloc(ARR_SIZE, sizeof(*n64), GFP_ATOMIC);
+                p64 = kcalloc(ARR_SIZE, sizeof(*p64), GFP_ATOMIC);
+                
+		if (!n16 || !c16 || !p16 || !n32 || !p32 || !ck32 || !c32 ||
+                        !n40 || !p40 || !ck40 || !c40 || 
+                        !n48 || !p48 || !ck48 || !c48 || !n64 || !p64) {
+			kfree(n16);
+			kfree(c16);
+			kfree(p16);
+                        kfree(n32);
+                        kfree(p32);
+                        kfree(ck32);
+                        kfree(c32);
+                        kfree(n40);
+                        kfree(p40);
+                        kfree(ck40);
+                        kfree(c40);
+                        kfree(n48);
+                        kfree(p48);
+                        kfree(ck48);
+                        kfree(c48);
+                        kfree(n64);
+                        kfree(p64);
+			pr_err("Out of memory while preallocating  SAIL");
+			goto error;
+		}
+
+		RCU_INIT_POINTER(s->N16, n16);
+		RCU_INIT_POINTER(s->P16, p16);
+		RCU_INIT_POINTER(s->C16, c16);
+
+                RCU_INIT_POINTER(s->N32, n32);
+                RCU_INIT_POINTER(s->P32, p32);
+                RCU_INIT_POINTER(s->CK32, ck32);
+                RCU_INIT_POINTER(s->C32, c32);
+
+                RCU_INIT_POINTER(s->N40, n40);
+                RCU_INIT_POINTER(s->P40, p40);
+                RCU_INIT_POINTER(s->CK40, ck40);
+                RCU_INIT_POINTER(s->C40, c40);
+                
+                RCU_INIT_POINTER(s->N48, n48);
+                RCU_INIT_POINTER(s->P48, p48);
+                RCU_INIT_POINTER(s->CK48, ck48);
+                RCU_INIT_POINTER(s->C48, c48);
+
+                RCU_INIT_POINTER(s->N64, n64);
+                RCU_INIT_POINTER(s->P64, p64); 
+                
+//		synchronize_rcu();
+	}
+
+	/*Eextract 16 bits from MSB.*/
+	n16_idx = key >> 48;
+
+	if (prefix_len <= 16) {
+		/*All the leafs in level 1~16 will be stored in level 16.*/
+		num_leafs = 1U << (16 - prefix_len);
+		for (i = 0; i < num_leafs; i++) {
+			/*Longer prefix exists*/
+			if (s->P16[n16_idx + i] > prefix_len)
+				continue;
+			s->N16[n16_idx + i] = netdev_index;
+			s->P16[n16_idx + i] = prefix_len;
+		}
+		goto finish;
+	}
+
+	/* The length of the prefix is 21~64. So need to check if there is a
+	 * chunk for this prefix in level 24. C16[..] = 0 indicates that there
+	 * is no chunk, so need to insert one. The insertion works as
+	 * following:
+	 * 1. Calculate the chunk ID to level 24 from C16. The Chunk ID
+	 * indicates where the new chunk should be inserted.
+	 * 2. Insert a new chunk in level 24
+	 * 3. Update the C16[] based on the newly inserted chunk
+	 */
+	if (s->C16[n16_idx] == 0) {
+		/*Step 1*/
+		chunk_id = calc_ckid24_from_C16(s->C16, LEVEL16_SIZE, n16_idx);
+		if (!chunk_id)
+			goto error;
+		/*Step 2*/
+		err = chunk_insert(s->N32, s->P32, s->CK32, &s->cnk32_count, chunk_id, CNK_16);
+		if (err) {
+                        pr_err("Could not insert chunk to level 32");
+			goto error;
+                }
+		/*Step 3*/
+		err = update_C16(s->C16, LEVEL16_SIZE, n16_idx, chunk_id);
+		if (err)
+			goto error;
+	}
+
+	/*Extract bit 17~32 from MSB and calculate index to level 32*/
+	n_idx = (s->C16[n16_idx] - 1) * CNK_16 + ((key >> 32) & 0XFFFF);
+	ck_idx = s->C16[n16_idx] - 1;
+	ck_off = (key >> 32) & 0XFFFF;
+
+	if (prefix_len <= 32) {
+		/*All the leafs in level 17~32 will be stored in level 32.*/
+		num_leafs = 1U << (32 - prefix_len);
+		for (i = 0; i < num_leafs; i++) {
+			/*Longer prefix exists*/
+                        if (s->P32[n_idx + i] > prefix_len)
+                                continue;
+                        s->N32[n_idx + i] = netdev_index;
+                        s->P32[n_idx + i] = prefix_len;
+		}
+		goto finish;
+	}
+
+
+        /* The length of the prefix is 33-64, but there is no chunk for
+         * this prefix in the next level. So do the same as before
+         */
+        part_idx = ck_off / 64;
+        part_off = ck_off % 64;
+        if (!(s->CK32[ck_idx].bitmap[part_idx] & (1ULL << part_off))) {
+                chunk_id = calc_ckid(s->CK32, ck_idx, ck_off, s->C32);
+                if (!chunk_id)
+                        goto error;
+                err = chunk_sm_insert(s->N40, s->P40, s->CK40, &s->cnk40_count, chunk_id, CNK_8);
+		if (err) {
+                        pr_err("Could not insert chunk to level 40");
+			goto error;
+                }
+                err = update_ck_c(s->CK32, ck_idx, ck_off,
+                                      s->C32, s->cnk32_count, chunk_id);
+                if (err)
+                        goto error;
+        }
+
+        c_idx = calc_c_idx(&(s->CK32[ck_idx]), ck_off);
+        /*Extract 16 more bits and calculate index to level 48, 64,..*/
+        n_idx = (s->C32[c_idx] - 1) * CNK_8 + ((key >> 24) & 0XFF);
+        ck_idx = s->C32[c_idx] - 1;
+        ck_off = (key >> 24) & 0XFF;
+
+        if (prefix_len <= 40) {
+                /*All the leafs in level 33~48 will be stored in level 48.*/
+                num_leafs = 1U << (40 - prefix_len);
+                for (i = 0; i < num_leafs; i++) {
+                        /*Longer prefix exists*/
+                        if (s->P40[n_idx + i] > prefix_len)
+                                continue;
+                        s->N40[n_idx + i] = netdev_index;
+                        s->P40[n_idx + i] = prefix_len;
+                }
+                goto finish;
+        }
+        
+        /* The length of the prefix is 33-64, but there is no chunk for
+         * this prefix in the next level. So do the same as before
+         */
+        part_idx = ck_off / 64;
+        part_off = ck_off % 64;
+        if (!(s->CK40[ck_idx].bitmap[part_idx] & (1ULL << part_off))) {
+                chunk_id = calc_ckid_sm(s->CK40, ck_idx, ck_off, s->C40);
+                if (!chunk_id)
+                        goto error;
+                err = chunk_sm_insert(s->N48, s->P48, s->CK48, &s->cnk48_count, chunk_id, CNK_8);
+		if (err) {
+                        pr_err("Could not insert chunk to level 48");
+			goto error;
+                }
+                err = update_ck_sm_c(s->CK40, ck_idx, ck_off,
+                                      s->C40, s->cnk40_count, chunk_id);
+                if (err)
+                        goto error;
+        }
+
+        c_idx = calc_c_idx_sm(&(s->CK40[ck_idx]), ck_off);
+        /*Extract 16 more bits and calculate index to level 48, 64,..*/
+        n_idx = (s->C40[c_idx] - 1) * CNK_8 + ((key >> 16) & 0XFF);
+        ck_idx = s->C40[c_idx] - 1;
+        ck_off = (key >> 16) & 0XFF;
+
+        if (prefix_len <= 48) {
+                /*All the leafs in level 33~48 will be stored in level 48.*/
+                num_leafs = 1U << (48 - prefix_len);
+                for (i = 0; i < num_leafs; i++) {
+                        /*Longer prefix exists*/
+                        if (s->P48[n_idx + i] > prefix_len)
+                                continue;
+                        s->N48[n_idx + i] = netdev_index;
+                        s->P48[n_idx + i] = prefix_len;
+                }
+                goto finish;
+        }
+        
+        
+        /* The length of the prefix is 49-64, but there is no chunk for
+         * this prefix in the next level. So do the same as before
+         */
+        part_idx = ck_off / 64;
+        part_off = ck_off % 64;
+        if (!(s->CK48[ck_idx].bitmap[part_idx] & (1ULL << part_off))) {
+                chunk_id = calc_ckid_sm(s->CK48, ck_idx, ck_off, s->C48);
+                if (!chunk_id)
+                        goto error;
+                err = chunk_insert(s->N64, s->P64, NULL, &s->cnk64_count, chunk_id, CNK_16);
+		if (err) {
+                        pr_err("Could not insert chunk to level 64");
+			goto error;
+                }
+                err = update_ck_sm_c(s->CK48, ck_idx, ck_off,
+                                      s->C48, s->cnk48_count, chunk_id);
+                if (err)
+                        goto error;
+        }
+
+        c_idx = calc_c_idx_sm(&(s->CK48[ck_idx]), ck_off);
+        /*Extract 16 more bits and calculate index to level 48, 64,..*/
+        n_idx = (s->C48[c_idx] - 1) * CNK_16 + (key & 0XFFFF);
+        ck_idx = s->C32[c_idx] - 1;
+        ck_off = key & 0XFFFF;
+
+        if (prefix_len <= 64) {
+                /* All the leafs in level 49~64 will be stored in level 64.*/
+                num_leafs = 1U << (64 - prefix_len);
+                for (i = 0; i < num_leafs; i++) {
+                        /*Longer prefix exists*/
+                        if (s->P64[n_idx + i] > prefix_len)
+                                continue;
+                        s->N64[n_idx + i] = netdev_index;
+                        s->P64[n_idx + i] = prefix_len;
+                }
+                goto finish;
+        }       
+
+error:
+	pr_err("Something went wrong in route insertion");
+finish:
+	spin_unlock(&s->lock);
+	return err;
+}
+
+int sail_lookup(const struct sail *s, const u64 key, struct net_device **dev)
+{
+        int j;
+        u8 bitshifts;
+	u8 *n16, *n;
+	u16 *c16, *c;
+	struct chunk *ck;        
+        struct chunk_small *ck_sm; /*Small chunk*/
+	u8 netdev_index = s->def_nh;
+	u16 n16_idx;/*Index to N16 and C16*/
+	u32 n_idx;/*Index to N and C*/
+	u32 ck_idx;/*Index to CK*/
+	u64 ck_off;/*Index to CK*/
+	u64 c_idx;/*Index to C*/
+	u32 part_idx, part_off;
+
+	rcu_read_lock();
+
+	/*extract 16 bits from LSB*/
+	n16_idx = key >> 48;
+	n16 = rcu_dereference(s->N16);
+	if (unlikely(!n16))
+		goto finish;
+
+	if (likely(n16[n16_idx] != 0))
+		netdev_index = n16[n16_idx];
+
+	/*Check if there is a longer prefix; if yes, extract bit 17~32
+	 *  and calculate index to N32
+	 */
+	c16 = rcu_dereference(s->C16);
+	if (likely(c16[n16_idx] != 0)) {
+		ck_idx = c16[n16_idx] - 1;
+		ck_off = (key >> 32) & 0XFFFF;
+		n_idx = ck_idx * CNK_16 + ck_off;
+	} else {
+		goto finish;
+	}
+        
+        /*Find corresponding next-hop in level 32*/
+        n = rcu_dereference(s->N32);
+        if (likely(n[n_idx] != 0))
+                netdev_index = n[n_idx];
+
+        /*Check if there is a longer prefix; if yes, extract more 16 bits
+         * from MSB and calculate index to N48
+         */
+        ck = rcu_dereference(s->CK32);
+        part_idx = ck_off / 64;
+        part_off = ck_off % 64;
+        
+        if (likely(ck[ck_idx].bitmap[part_idx] & (1ULL << part_off))) {
+                c = rcu_dereference(s->C32);
+                c_idx = ck[ck_idx].start_index[part_idx] +
+                        POPCNT_OFF(ck[ck_idx].bitmap[part_idx], part_off);
+                ck_idx = c[c_idx] - 1;
+                ck_off = (key >> 24) & 0XFF;
+                n_idx = ck_idx * CNK_8 + ck_off;
+        } else {
+                goto finish;
+        }
+
+        n = rcu_dereference(s->N40);
+        if (likely(n[n_idx] != 0))
+                netdev_index = n[n_idx];
+        
+        /*Check if there is a longer prefix; if yes, extract more 16 bits
+         * from MSB and calculate index to N48
+         */
+        ck_sm = rcu_dereference(s->CK40);
+        part_idx = ck_off / 64;
+        part_off = ck_off % 64;
+        if (likely(ck_sm[ck_idx].bitmap[part_idx] & (1ULL << part_off))) {
+                c = rcu_dereference(s->C40);
+                c_idx = ck_sm[ck_idx].start_index[part_idx] +
+                        POPCNT_OFF(ck_sm[ck_idx].bitmap[part_idx], part_off);
+                ck_idx = c[c_idx] - 1;
+                ck_off = (key >> 16) & 0XFF;
+                n_idx = ck_idx * CNK_8 + ck_off;
+        } else {
+                goto finish;
+        }
+
+        n = rcu_dereference(s->N48);
+        if (likely(n[n_idx] != 0))
+                netdev_index = n[n_idx];
+        
+        /*Check if there is a longer prefix; if yes, extract more 16 bits
+         * from MSB and calculate index to N64
+         */
+        ck_sm = rcu_dereference(s->CK48);
+        part_idx = ck_off / 64;
+        part_off = ck_off % 64;
+        if (likely(ck_sm[ck_idx].bitmap[part_idx] & (1ULL << part_off))) {
+                c = rcu_dereference(s->C48);
+                c_idx = ck_sm[ck_idx].start_index[part_idx] +
+                        POPCNT_OFF(ck_sm[ck_idx].bitmap[part_idx], part_off);
+                ck_idx = c[c_idx] - 1;
+                ck_off = key & 0XFFFF;
+                n_idx = ck_idx * CNK_16 + ck_off;
+        } else {
+                goto finish;
+        }
+
+        n = rcu_dereference(s->N64);
+        if (likely(n[n_idx] != 0))
+                netdev_index = n[n_idx];
+
+finish:
+	*dev = s->netdevs[netdev_index];
+	rcu_read_unlock();
+	return 0;
+}
